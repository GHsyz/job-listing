<%= simple_form_for @job do |f| %>

  <div class="form-job">
     <%= f.input :title, input_html: { class: "form-control"} %>
     <%= f.input :description, input_html: { class: "form-control"} %>
     <%= f.input :wage_lower_bound, :label => "薪资下限", input_html: { class: "form-control"} %>
     <%= f.input :wage_upper_bound, :label => "薪资上限",  input_html: { calss: "form-control"} %>
     <%= f.input :contact_email, :label => "联系方式",  input_html: { calss: "form-contrl"} %>
  </div>
  <%= f.submit "Submit", class: "btn btn-primary", data: { disable_with: "Submiting..." } %>
<% end %>
